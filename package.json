{
	"name": "onedash-react-input-form",
	"version": "2.1.10",
	"description": " OneDash. React form and input components",
	"author": "ChewbaccaCookie",
	"license": "MIT",
	"repository": "OneDash-DE/onedash-react-input-form",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.ts",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "microbundle --no-sourcemap --css-modules false --jsx React.createElement --format modern",
		"start": "npm run storybook",
		"lint": "eslint \"./src/**/*.{js,ts,tsx}\" --quiet",
		"format": "prettier --write src/**/*.{js,ts,tsx}",
		"release": "semantic-release",
		"install-all": "npm i && cd example && npm i",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook -o example"
	},
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/core": "7.14.6",
		"@babel/plugin-syntax-nullish-coalescing-operator": "7.8.3",
		"@semantic-release/git": "9.0.0",
		"@storybook/addon-actions": "6.2.9",
		"@storybook/addon-docs": "6.2.9",
		"@storybook/addon-essentials": "6.2.9",
		"@storybook/addon-links": "6.2.9",
		"@storybook/preset-create-react-app": "3.1.7",
		"@storybook/react": "6.2.9",
		"@types/node": "15.0.1",
		"@types/react": "17.0.14",
		"@types/react-dates": "21.8.3",
		"@types/react-dom": "17.0.9",
		"@types/react-select": "4.0.17",
		"@typescript-eslint/eslint-plugin": "4.22.0",
		"@typescript-eslint/parser": "4.22.0",
		"babel-eslint": "10.1.0",
		"babel-loader": "8.2.2",
		"conventional-changelog-conventionalcommits": "4.6.0",
		"cross-env": "7.0.3",
		"eslint": "7.25.0",
		"eslint-config-airbnb-typescript": "12.3.1",
		"eslint-config-prettier": "8.3.0",
		"eslint-config-standard": "16.0.3",
		"eslint-config-standard-react": "11.0.1",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-mdx": "1.13.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-promise": "5.1.0",
		"eslint-plugin-react": "7.23.2",
		"eslint-plugin-standard": "5.0.0",
		"husky": "7.0.0",
		"microbundle": "0.13.3",
		"npm-run-all": "4.1.5",
		"prettier": "2.2.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-scripts": "4.0.3",
		"sass": "1.32.12",
		"semantic-release": "17.4.4",
		"typescript": "4.2.4"
	},
	"files": [
		"dist"
	],
	"husky": {
		"hooks": {
			"pre-merge": "npm run install-all"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"@types/cleave.js": "^1.4.4",
		"@types/uuid": "^8.3.0",
		"cleave.js": "^1.6.0",
		"dayjs": "^1.10.4",
		"moment": "^2.29.1",
		"react-dates": "^21.8.0",
		"react-dropzone": "^11.3.2",
		"react-input-range": "^1.3.0",
		"react-select": "^4.3.0",
		"uuid": "^8.3.2"
	}
}
