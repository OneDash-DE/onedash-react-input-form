{
	"name": "onedash-react-input-form",
	"version": "2.2.11",
	"description": " OneDash. React form and input components",
	"author": "ChewbaccaCookie",
	"license": "MIT",
	"repository": "OneDash-DE/onedash-react-input-form",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.ts",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --no-sourcemap --css-modules false --format modern",
		"start": "npm run storybook",
		"lint": "tsc --noEmit && eslint \"./src/**/*.{js,ts,tsx}\" --quiet",
		"format": "prettier --write \"./src/**/*.{js,ts,tsx}\"",
		"release": "semantic-release",
		"install-all": "npm i && cd example && npm i",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/core": "7.18.6",
		"@babel/plugin-syntax-nullish-coalescing-operator": "7.8.3",
		"@semantic-release/git": "10.0.1",
		"@storybook/addon-actions": "6.5.9",
		"@storybook/addon-docs": "6.5.9",
		"@storybook/addon-essentials": "6.5.9",
		"@storybook/addon-links": "6.5.9",
		"@storybook/builder-webpack5": "6.5.9",
		"@storybook/manager-webpack5": "6.5.9",
		"@storybook/preset-create-react-app": "4.1.2",
		"@storybook/react": "6.5.9",
		"@types/node": "17.0.1",
		"@types/react": "17.0.43",
		"@types/react-dates": "21.8.3",
		"@types/react-dom": "17.0.14",
		"@types/react-select": "5.0.0",
		"@typescript-eslint/eslint-plugin": "5.30.3",
		"@typescript-eslint/parser": "5.30.3",
		"babel-eslint": "10.1.0",
		"babel-loader": "8.2.4",
		"conventional-changelog-conventionalcommits": "4.6.3",
		"cross-env": "7.0.3",
		"eslint": "8.18.0",
		"eslint-config-airbnb": "19.0.2",
		"eslint-config-airbnb-typescript": "16.2.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-standard": "16.0.3",
		"eslint-config-standard-react": "11.0.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-mdx": "1.17.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-promise": "6.0.0",
		"eslint-plugin-react": "7.30.1",
		"eslint-plugin-standard": "5.0.0",
		"husky": "7.0.4",
		"microbundle": "0.15.1",
		"npm-run-all": "4.1.5",
		"prettier": "2.7.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-scripts": "5.0.0",
		"sass": "1.53.0",
		"semantic-release": "19.0.0",
		"typescript": "4.7.4",
		"webpack": "5.73.0"
	},
	"files": [
		"dist"
	],
	"husky": {
		"hooks": {
			"pre-merge": "npm run install-all"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"@types/cleave.js": "^1.4.6",
		"@types/uuid": "^8.3.3",
		"cleave.js": "^1.6.0",
		"dayjs": "^1.10.7",
		"moment": "^2.29.1",
		"react-dates": "^21.8.0",
		"react-dropzone": "^11.4.2",
		"react-input-range": "^1.3.0",
		"react-select": "^5.2.1",
		"uuid": "^8.3.2"
	}
}
